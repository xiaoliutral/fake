{
  "Serilog": {
    "MinimumLevel": {
      "Default": "Debug",
      "Override": {
        "Microsoft.AspNetCore": "Debug"
      }
    },
    "Enrich": [
      "FromLogContext",
      "WithMachineName",
      "WithProcessId",
      "WithThreadId"
    ],
    "WriteTo": [
      {
        "Name": "Console"
      }
    ]
  },
  "App": {
    "Urls": [
      "http://*:5281"
    ],
    "CorsOrigins": [
      "http://localhost:5281"
    ]
  },
  "AllowedHosts": "*",
  "ConnectionStrings": {},
  "JwtSettings": {}
}