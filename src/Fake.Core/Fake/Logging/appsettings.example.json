{
    "Logging": {
        "LogLevel": {
            "Default": "Information",
            "Microsoft": "Warning",
            "Microsoft.Hosting.Lifetime": "Information"
        },
        "FeiShu": {
            "IsEnabled": true,
            "MinimumLevel": "Information",
            "FeiShuMinimumLevel": "Warning",
            "NotificationOptions": {
                "Webhook": "https://open.feishu.cn/open-apis/bot/v2/hook/your-webhook-key",
                "TitlePrefix": "生产环境告警",
                "Timeout": 20,
                "QueueCapacity": 500,
                "BatchSize": 10,
                "BatchIntervalSeconds": 5
            }
        }
    }
}